{
	"name": "archive-browser",
	"displayName": "Archive Browser",
	"description": "Browser archive files in VS Code",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.45.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:archive-browser.extract",
		"onCommand:archive-browser.menuExtract"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "archive-browser.extract",
				"title": "Extract Archive"
			},
			{
				"command": "archive-browser.menuExtract",
				"title": "Extract Files"
			}
		],
		"menus": {
			"explorer/context": [
				{
					"when": "isFileSystemResource",
					"command": "archive-browser.menuExtract"
				}
			]
		}
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"lint": "eslint src --ext ts",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.1",
		"@types/mocha": "^7.0.2",
		"@types/node": "^13.13.10",
		"@types/vscode": "^1.45.0",
		"@typescript-eslint/eslint-plugin": "^2.30.0",
		"@typescript-eslint/parser": "^2.30.0",
		"eslint": "^6.8.0",
		"glob": "^7.1.6",
		"mocha": "^7.1.2",
		"typescript": "^3.8.3",
		"vscode-test": "^1.3.0"
	},
	"dependencies": {
		"os": "^0.1.1",
		"typescript-logging": "^0.6.4"
	}
}
